 <equation xml:id="eq.fermatphrase">
      <title>Fermat's Last Theorem</title>
   <mathphrase>x<superscript>n</superscript>
    + y<superscript>n</superscript>
  ≠ z<superscript>n</superscript>
    ∀ n ≠ 2</mathphrase>
  </equation>
    
